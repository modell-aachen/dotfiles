qorbo() {
    echo "Sometimes the application hangs, just press Ctrl+D to continue"

    sudo systemctl stop qwiki.service events.qwiki.service

    cd $FOSWIKI_ROOT/bin

    sudo -u www-data -s \
        source /etc/default/qwiki && \
        TENANT_MANAGEMENT_AUTH_TOKEN=$TENANT_MANAGEMENT_AUTH_TOKEN \
        TENANT_MANAGEMENT_SUPPORTED_DOMAINS=$TENANT_MANAGEMENT_SUPPORTED_DOMAINS \
        morbo -w /opt/repos/QwikiContrib/core/lib --listen http://localhost:1337 ./qwiki

    cd - > /dev/null
}
